{
    "name": "@kaave/site",
    "version": "1.0.0",
    "description": "kaave&#39;s website",
    "author": "kaave",
    "private": true,
    "scripts": {
        "dev": "run-p dev:*",
        "dev:nuxt": "nuxt",
        "dev:mock": "ts-node -P ./tsconfig.ts-node.json mock",
        "build": "nuxt build",
        "start": "nuxt start",
        "generate": "nuxt generate",
        "typecheck": "tsc --noEmit -p ./tsconfig.production.json",
        "lint-staged": "lint-staged",
        "lint": "run-s lint:*",
        "lint:script": "eslint --ignore-path .gitignore 'src/**/*.{js,ts}'",
        "lint:style": "stylelint --ignore-path .gitignore './src/**/*.scss'",
        "lint:vue": "run-s lint:vue:*",
        "lint:vue:script": "eslint --ignore-path .gitignore './src/**/*.vue'",
        "lint:vue:style": "stylelint --config .stylelintrc.vue.js --ignore-path .gitignore './src/**/*.vue'",
        "fix": "run-s fix:*",
        "fix:script": "eslint --fix --ignore-path .gitignore './src/**/*.{js,ts}'",
        "fix:style": "stylelint --fix --ignore-path .gitignore './src/**/*.scss'",
        "fix:prettier": "prettier --write --ignore-path .gitignore './src/**/*.{ts,js,scss,md,json}'",
        "fix:vue": "run-s fix:vue:*",
        "fix:vue:script": "eslint --fix --ignore-path .gitignore './src/**/*.vue'",
        "fix:vue:prettier": "prettier --write --ignore-path .gitignore './src/**/*.vue'"
    },
    "browserslist": [
        "ie 11",
        "ios >= 10.3",
        "> 1% in JP",
        "not ie <= 10",
        "not chrome 49"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "run-s typecheck lint-staged"
        }
    },
    "resolutions": {
        "node-fetch": "^2.6.0"
    },
    "dependencies": {
        "@kaave/vue-disortion-carousel": "^0.1.1",
        "@nuxtjs/apollo": "^4.0.0-rc7",
        "@nuxtjs/axios": "^5.5.4",
        "@nuxtjs/dotenv": "^1.3.0",
        "@nuxtjs/google-analytics": "^2.2.0",
        "@nuxtjs/markdownit": "^1.2.5",
        "@nuxtjs/sitemap": "^1.2.0",
        "@nuxtjs/style-resources": "^0.1.2",
        "cross-env": "^5.2.0",
        "date-fns": "^1.30.1",
        "dotenv": "^8.0.0",
        "gsap": "^2.1.3",
        "lodash": "^4.17.11",
        "markdown-it-attrs": "^2.4.1",
        "markdown-it-prism": "^2.0.2",
        "nuxt": "^2.8.1",
        "nuxt-webfontloader": "^1.1.0",
        "sanitize.css": "^10.0.0",
        "ts-node": "^8.3.0",
        "tslib": "^1.10.0",
        "vue-observe-visibility": "^0.4.4"
    },
    "devDependencies": {
        "@fullhuman/postcss-purgecss": "^1.2.0",
        "@mapbox/stylelint-processor-arbitrary-tags": "^0.2.0",
        "@nuxt/typescript": "^2.8.1",
        "@nuxtjs/eslint-config": "^0.0.1",
        "@types/dotenv": "^6.1.1",
        "@types/fibers": "^0.0.30",
        "@types/gsap": "^1.20.2",
        "@types/markdown-it": "^0.0.7",
        "@types/node": "^10",
        "@types/sass": "^1.16.0",
        "@types/webpack": "^4.4.34",
        "@types/webpack-env": "^1.13.9",
        "@typescript-eslint/eslint-plugin": "^1.11.0",
        "@typescript-eslint/parser": "^1.11.0",
        "cssnano": "^4.1.10",
        "eslint": "^5.0.1",
        "eslint-config-airbnb-base": "^13.2.0",
        "eslint-config-prettier": "^6.0.0",
        "eslint-config-standard": ">=12.0.0",
        "eslint-import-resolver-typescript": "^1.1.1",
        "eslint-plugin-import": ">=2.18.0",
        "eslint-plugin-jest": ">=22.7.2",
        "eslint-plugin-node": ">=9.1.0",
        "eslint-plugin-nuxt": "^0.4.3",
        "eslint-plugin-prettier": "3.1.0",
        "eslint-plugin-promise": ">=4.2.1",
        "eslint-plugin-standard": ">=4.0.0",
        "eslint-plugin-vue": "^5.2.3",
        "fibers": "^4.0.1",
        "husky": "^3.0.0",
        "imagemin-gif2webp": "^2.0.0",
        "imagemin-mozjpeg": "^8.0.0",
        "imagemin-pngquant": "^8.0.0",
        "imagemin-webp": "^5.1.0",
        "json-graphql-server": "^2.1.3",
        "lint-staged": "^9.1.0",
        "nodemon": "^1.19.1",
        "npm-run-all": "^4.1.5",
        "nuxt-imagemin": "^1.0.0",
        "postcss-calc": "^7.0.1",
        "postcss-color-hex-alpha": "^5.0.3",
        "postcss-custom-properties": "^9.0.1",
        "postcss-flexbugs-fixes": "^4.1.0",
        "postcss-url": "^8.0.0",
        "prettier": "1.18.2",
        "sass": "^1.22.0",
        "sass-loader": "^7.1.0",
        "stylelint": "^10.1.0",
        "stylelint-config-prettier": "^5.2.0",
        "stylelint-config-recess-order": "^2.0.2",
        "stylelint-config-recommended-scss": "^3.3.0",
        "stylelint-scss": "^3.9.1"
    }
}
